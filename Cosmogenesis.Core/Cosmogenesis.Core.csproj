<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

    <PackageId>Cosmogenesis.Core</PackageId>
    <Version>0.0.5</Version>
    <Authors>Josh</Authors>
    <Company />
    <Product>Cosmogenesis</Product>
    <Description>The core components required in order to create a CosmosDB project generated by Cosmogenesis.</Description>
    <PackageTags>Cosmos, CosmosDB, Source Generator</PackageTags>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/joshlang/Cosmogenesis</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos" Version="3.17.0" />
    <PackageReference Include="System.Text.Json" Version="5.0.1" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
